<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lakiosktest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.lakiosktest.BuyScreenActivity"
            android:label="@string/title_activity_buy_screen" 
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.lakiosktest.MainActivity" />
        </activity>
        <activity
            android:name="com.example.lakiosktest.SellScreenActivity"
            android:label="@string/title_activity_buy_screen" 
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.lakiosktest.MainActivity" />
        </activity>
        <activity
            android:name="com.example.lakiosktest.BuyerInformationActivity"
            android:label="@string/title_activity_address"
            android:parentActivityName="ccom.example.lakiosktest.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.lakiosktest.MainActivity" />
        </activity>

         <activity
            android:name="com.example.lakiosktest.BuyerSelectionActivity"
            android:label="@string/title_activity_selection"
            android:parentActivityName="com.example.lakiosktest.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.lakiosktest.MainActivity" />
        </activity>
        <activity
            android:name="com.example.lakiosktest.LoginActivity"
            android:label="@string/title_activity_login"
            android:parentActivityName="com.cash2btc.bitcoindave.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.lakiosktest.MainActivity" />
        </activity>
         <activity
            android:name="com.example.lakiosktest.Redemption_Activity"
            android:label="@string/title_activity_buy_qr_"
            android:parentActivityName="com.example.lakiosktest.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.lakiosktest.MainActivity" />
        </activity>
    </application>

</manifest>