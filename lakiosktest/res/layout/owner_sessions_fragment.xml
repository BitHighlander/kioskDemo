            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="label" />

        <TextView
            android:id="@+id/owner_label8"
            style="@style/Value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="label" />

        <TextView
            android:id="@+id/owner_label9"
            style="@style/Value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="label" />

        <TextView
